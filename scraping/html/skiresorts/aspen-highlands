                url: action, // point to server-side PHP script
                dataType: 'text',  // what to expect back from the PHP script, if anything
                cache: false,
                contentType: false,
                processData: false,
                data: formData,
                type: 'post',
                success: function(res){
                    $('#notification').removeClass('loading')
                    $('#notification').html($('#response-content',res));
                }
            });


